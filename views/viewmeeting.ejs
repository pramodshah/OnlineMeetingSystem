<%- include('./partials/headertwo') %>

<div class="container" style="margin-top:5%;width:100%">
    <div class="row">
        <div class="col-sm-4"></div>
        <div class="col-sm-4">
            <div class="card border-secondary mb-3" style="max-width: 20rem;">
                <div class="card-header">Your Meeting</div>
                <div class="card-body">
                  <p class="card-text"><span style="color:crimson;padding-right:20px;font-size:1rem;">Title </span><%=meeting.title%></p>
                  <p class="card-text"><span style="color:crimson;padding-right:20px;font-size:1rem;">Meeting With: </span><%=meeting.meetingwith%></p>
                  <p class="card-text"><span style="color:crimson;padding-right:20px;font-size:1rem;">Meeting Date: </span><%=meeting.date%></p>
                  <p class="card-text"><span style="color:crimson;padding-right:20px;font-size:1rem;">Meeting Time:  </span><%=meeting.time%></p>
                </div> 
            </div>

        </div>
        <div class="col-sm-4"></div>
        

    </div>
    

</div>
